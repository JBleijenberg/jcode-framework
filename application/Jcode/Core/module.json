{
    "module": {
        "name": "Jcode_Core",
        "version": "2.0.1",
        "active": true,
        "code": "core",
        "controller":
            {
                "class": "Jcode\\Core\\Controller",
                "frontname": "core"
            },
        "design": [
            {
                "path": "core_index_index",
                "template": "core/index/home.html"
            },
            {
                "path": "core_index_2",
                "template": "core/index/2.html"
            }
        ],
        "observers": [
            {
                "name": "addFlowHelpers",
                "handle": "layout_load_helpers_after",
                "class": "Jcode\\Core\\Model\\Observer",
                "method": "addFlowHelpers"
            },
            {
                "name": "flowInitBefore",
                "handle": "layout_flow_init_before",
                "class": "Jcode\\Core\\Model\\Observer",
                "method": "flowInitBefore"
            }
        ]
    }
}